{
  "functions": [
    {
      "source": "data_collection",
      "codebase": "data_collection",
      "runtime": "python312",
      "ignore": [
        "venv",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local",
        "pytest.ini"
      ]
    }
  ],
  "hosting": {
    "public": "dashboard-v2/out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}